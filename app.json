{
  "expo": {
    "name": "gopoker",
    "slug": "gopoker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B2430"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.harpreetgh.pokerfriends"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B2430"
      },
      "package": "com.harpreetgh.pokerfriends"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
